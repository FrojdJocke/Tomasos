@model TomasosASP.ViewModels.AccountModel
@{
    ViewData["Title"] = "AccountEdit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row container-fluid">
    <div class="col-sm-8 col-sm-offset-2 home-col-logoff content" align="center">

        <h2>Uppdatera Kundinformation</h2>
        <br/>

        <form asp-controller="Account" asp-action="Register" method="post">
            @Html.Partial("_UserRegistration", Model.Customer)

            <input type="hidden" asp-for="Customer.Losenord" value="@Model.Customer.Losenord"/> <br/>
            <span asp-validation-for="Customer.Losenord"></span>

            <br/>
            <br/>

            <input id="register" type="submit" value="Registrera"/>
            <br/>
            <br/>


        </form>

    </div>


</div>